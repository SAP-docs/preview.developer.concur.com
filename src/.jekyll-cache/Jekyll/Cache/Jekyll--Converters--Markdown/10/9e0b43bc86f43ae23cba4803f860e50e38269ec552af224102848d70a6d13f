      }
    ],
    "name": "string",
    "openAndClosedFiscalPeriods": [
      {
        "currentPeriod": true,
        "endDate": "2019-08-24",
        "fiscalPeriodStatus": "OPEN",
        "fiscalYearSyncGuid": "string",
        "id": "string",
        "name": "string",
        "periodType": "MONTHLY",
