      }
    ],
    "quarterlyFiscalPeriods": [
      {
        "currentPeriod": true,
        "endDate": "2019-08-24",
        "fiscalPeriodStatus": "OPEN",
        "fiscalYearSyncGuid": "string",
        "id": "string",
        "name": "string",
        "periodType": "MONTHLY",
        "spendDate": "2019-08-24",
        "startDate": "2019-08-24",
        "syncGuid": "string"
      }
    ],
    "startDate": "2019-08-24",
    "status": "OPEN",
    "syncGuid": "string",
