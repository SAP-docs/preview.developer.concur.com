  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Format</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><code class="language-plaintext highlighter-rouge">vendors</code></td>
      <td>-</td>
      <td><code class="language-plaintext highlighter-rouge">body</code></td>
      <td>The vendor details.</td>
    </tr>
  </tbody>
</table>

