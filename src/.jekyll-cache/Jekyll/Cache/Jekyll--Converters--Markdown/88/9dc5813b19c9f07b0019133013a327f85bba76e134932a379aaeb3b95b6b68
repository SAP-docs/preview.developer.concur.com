    <span class="nv">$response</span> <span class="o">=</span> <span class="nv">$client</span><span class="o">-&gt;</span><span class="nf">request</span><span class="p">(</span><span class="s1">'PUT'</span><span class="p">,</span><span class="s1">'/awsqa-event-mgmt.nonprod.cnqr.tech/v4/subscriptions/webhook'</span><span class="p">,</span> <span class="k">array</span><span class="p">(</span>
