    <span class="nt">&lt;records-succeeded&gt;</span>2<span class="nt">&lt;/records-succeeded&gt;</span>
    <span class="nt">&lt;records-failed&gt;</span>0<span class="nt">&lt;/records-failed&gt;</span>
