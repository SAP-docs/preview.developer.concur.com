<span class="k">try</span> <span class="p">{</span>
    <span class="nv">$response</span> <span class="o">=</span> <span class="nv">$client</span><span class="o">-&gt;</span><span class="nf">request</span><span class="p">(</span><span class="s1">'GET'</span><span class="p">,</span><span class="s1">'https://us.api.concursolutions.com/profile/identity/v4/users/{id}/'</span><span class="p">,</span> <span class="k">array</span><span class="p">(</span>
