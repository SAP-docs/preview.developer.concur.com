    <span class="s1">'concur-uid'</span> <span class="o">=&gt;</span> <span class="s1">'string'</span><span class="p">,</span>
    <span class="s1">'Authorization'</span> <span class="o">=&gt;</span> <span class="s1">'API_KEY'</span><span class="p">,</span>
<span class="p">);</span>

